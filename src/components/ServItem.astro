---
import { services } from "@cv";
---
{
    services.map(({ title, icon, summary})=> {
        return (
            <div class="service-item padd-15">
                <div class="service-item-inner">
                    <div class="icon">
                        <i class={icon}></i>
                    </div>
                    <h4>{title}</h4>
                    <p>{summary}</p>
                </div>
            </div>
        );
    })
}